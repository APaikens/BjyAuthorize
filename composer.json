{
    "name":         "kokspflanze/bjy-authorize",
    "description":  "Zend\\Acl based firewall system for ZF2/3 dispatch protection",
    "type":         "library",
    "license":      "BSD-3-Clause",
    "homepage":     "https://github.com/kokspflanze/BjyAuthorize",
    "keywords": [
        "zf2",
        "zf3",
        "acl",
        "zfc-user"
    ],
    "authors": [
        {
            "name":      "Ben Youngblood",
            "email":     "bx.youngblood@gmail.com",
            "homepage":  "http://bjyoungblood.com/",
            "role":      "Developer"
        },
        {
            "name":      "Marco Pivetta",
            "email":     "ocramius@gmail.com",
            "homepage":  "http://ocramius.github.com/",
            "role":      "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "zendframework/zend-permissions-acl": "^2.7",
        "zendframework/zend-mvc":             "^3.1.1",
        "zendframework/zend-eventmanager":    "^3.2.1",
        "zendframework/zend-servicemanager":  "^3.4",
        "zendframework/zend-http":            "^2.10.0",
        "zendframework/zend-view":            "^2.11.2",
        "zendframework/zend-authentication":  "^2.7",
        "zendframework/zend-cache":           "^2.8.2"
    },
    "require-dev": {
        "phpunit/phpunit":                    "^7.5.12",
        "zendframework/zend-db":              "^2.5",
        "doctrine/common":                    "^2.10",
        "squizlabs/php_codesniffer": "^2.9.2",
        "zendframework/zend-developer-tools": "^1.2.3",
        "satooshi/php-coveralls":             "^0.6"
    },
    "suggests": {
        "zendframework/zend-developer-tools": "if you need to see current authorization details while developing",
        "zf-commons/zfc-user":                "zfc-user provides a good default setup to get started with bjyauthorize",
        "zf-commons/zfc-user-doctrine-orm":   "If you want to use BjyAuthorize with Doctrine ORM",
        "zf-commons/zfc-user-doctrine-mongo-odm": "If you want to use BjyAuthorize with Doctrine ODM"
    },
    "autoload": {
        "psr-4": {
            "BjyAuthorize\\": "src/BjyAuthorize"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BjyAuthorizeTest\\": "tests/BjyAuthorizeTest"
        }
    }
}
